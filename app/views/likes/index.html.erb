<p id="notice"><%= notice %></p>

<h1>Listing Likes</h1>

<table>
  <thead>
    <tr>
      <th>Number of like</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <table border="1px">
        <tr>
            <th><div class="filterBy"><%= link_to "Like", :update => "projects", :url => { :action => "sortTable", :filter => "Likes" }, :remote =>   true %></div></th>
            <th>Content</th>
        </tr>
        <% @likes.each do |like| %>
        <tr>
            <td><%= like.number_of_like %></td>
            <td>Ajax</td>
        </tr>
        <% end %>
    </table>
    
  </tbody>
</table>

<br>

<%= link_to 'New Like', new_like_path %>
